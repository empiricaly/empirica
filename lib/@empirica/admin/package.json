{
  "name": "@empirica/admin",
  "version": "1.0.0-beta.2",
  "description": "Empirica Admin",
  "main": "src/index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empiricaly/empirica.git"
  },
  "keywords": [
    "empirica",
    "experiment",
    "research",
    "multiplayer",
    "real-time",
    "behavioral",
    "social-science",
    "psychology"
  ],
  "author": "Nicolas Paton <nicolas.paton@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/empiricaly/empirica/issues"
  },
  "volta": {
    "node": "16.4.1",
    "yarn": "1.22.15",
    "npm": "7.19.1"
  },
  "homepage": "https://github.com/empiricaly/empirica#readme",
  "files": [
    "*"
  ],
  "np": {
    "contents": "pkg"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "pika build",
    "prerelease": "yarn run build",
    "release:next": "np --tag next --contents pkg/",
    "release": "np --tag latest --contents pkg/",
    "postversion": "cd pkg && yarn version --new-version $npm_package_version --no-git-tag-version --no-commit-hooks"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "pika-plugin-package.json",
        {
          "+volta": "^",
          "+author": "^",
          "+type": "^",
          "-devDependencies": {}
        }
      ],
      [
        "@pika/plugin-build-types"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-deno"
      ]
    ]
  },
  "devDependencies": {
    "@pika/cli": "0.2.0",
    "@pika/pack": "0.5.0",
    "@pika/plugin-build-deno": "0.9.2",
    "@pika/plugin-build-node": "0.9.2",
    "@pika/plugin-build-types": "0.9.2",
    "@pika/plugin-build-web": "0.9.2",
    "@pika/plugin-ts-standard-pkg": "0.9.2",
    "np": "7.5.0",
    "pika-plugin-package.json": "1.0.2",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@empirica/tajriba": "latest",
    "events": "3.3.0",
    "minimist": "1.2.5",
    "rand-seed": "1.0.1"
  }
}
