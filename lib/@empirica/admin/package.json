{
  "name": "@empirica/admin",
  "version": "1.0.0-beta.2",
  "description": "Empirica Admin",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.js",
      "require": "./dist/main.js"
    }
  },
  "targets": {
    "module": {
      "includeNodeModules": ["rand-seed"]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empiricaly/empirica.git"
  },
  "keywords": [
    "empirica",
    "experiment",
    "research",
    "multiplayer",
    "real-time",
    "behavioral",
    "social-science",
    "psychology"
  ],
  "author": "Nicolas Paton <nicolas.paton@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/empiricaly/empirica/issues"
  },
  "volta": {
    "node": "16.15.0",
    "npm": "8.10.0"
  },
  "engines": {
    "node": ">= 16.15.0"
  },
  "context": "node",
  "homepage": "https://github.com/empiricaly/empirica#readme",
  "files": [
    "*"
  ],
  "np": {
    "contents": "pkg"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "parcel build",
    "prerelease": "npm run build",
    "release:next": "np --tag next --contents pkg/",
    "release": "np --tag latest --contents pkg/",
    "postversion": "cd pkg && yarn version --new-version $npm_package_version --no-git-tag-version --no-commit-hooks"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.5.0",
    "@parcel/transformer-typescript-types": "^2.5.0",
    "@types/node": "17.0.35",
    "np": "7.5.0",
    "parcel": "2.5.0",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@empirica/tajriba": "latest",
    "events": "3.3.0",
    "minimist": "1.2.6",
    "rand-seed": "1.0.1"
  }
}
