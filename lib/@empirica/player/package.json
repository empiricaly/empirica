    "postversion": "cd pkg && npm version $npm_package_version --git-tag-version false --commit-hooks false"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.5.0",
    "@parcel/transformer-typescript-types": "^2.5.0",
    "@types/node": "17.0.35",
    "@types/react": "17.0.20",
    "np": "^7.6.1",
    "parcel": "2.5.0",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@empirica/tajriba": "latest",
    "@react-hook/mouse-position": "4.1.3",
    "events": "^3.3.0",
    "svelte": "^3.40.2"
  }
