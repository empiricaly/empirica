{
  "name": "empirica-server-demo",
  "private": true,
  "scripts": {
    "dev": "node --trace-warnings --enable-source-maps src/index.js",
    "build": "rsync -aP --exclude=node_modules --exclude=*.js -m src/* dist/ && esbuild src/index.js --platform=node --define:process.env.NODE_ENV='\"production\"' --bundle --minify --outfile=dist/index.js",
    "serve": "node --trace-warnings --enable-source-maps index.js"
  },
  "volta": {
    "node": "16.4.1",
    "yarn": "1.22.15",
    "npm": "7.19.1"
  },
  "dependencies": {
    "@empirica/admin": "latest",
    "minimist": "1.2.5",
    "rand-seed": "1.0.1"
  },
  "resolutions": {
    "graphql": "15.5.0"
  },
  "devDependencies": {
    "esbuild": "0.14.25"
  }
}
